{
    "version": "0.2.0",
    "configurations": [
       {
          "name": "Attach",
          "port": 9229,
          "request": "attach",
          "skipFiles": [
             "<node_internals>/**"
          ],
          "type": "pwa-node",

       },
       {
          "name": "(lldb) Attach",
          "type": "cppdbg",
          "request": "attach",
          "program": "/Users/andrewshand/.nvm/versions/node/v12.9.1/bin/node",
          "processId": "${command:pickProcess}",
          "MIMode": "lldb"
       },
      {
         "name": "(Windows) Launch",
         "type": "cppvsdbg",
         "request": "launch",
         "program": "${workspaceFolder}/node_modules/electron/dist/electron.exe",
         "args": [
            "${workspaceFolder}/dist/main/main.js",
            "--inspect=9229"
         ],
         "stopAtEntry": false,
         "cwd": "${workspaceFolder}",
         "environment": [
            {"name": "ELECTRON_RUN_AS_NODE", "value": ""},
            {"name": "NODE_ENV", "value": "dev"},
            {"name": "DEBUG", "value": "true"}
         ],
         "console": "externalTerminal"
      },
      {
         "type": "lldb",
         "request": "launch",
         "name": "test.js",
         "preLaunchTask": "npm: buildc:dev",
         "program": "/Users/andrewshand/.nvm/versions/node/v12.9.1/bin/node",
         "args": [
            "/Users/andrewshand/Github/electron-quick-start-typescript/local/tests/keyboard.js"
         ]
      }
   ],
   "compounds": [
      {
        "name": "C++/node",
        "configurations": ["(Windows) Launch", "Attach"],
        "preLaunchTask": "npm: buildc:dev"
      }
    ]
}