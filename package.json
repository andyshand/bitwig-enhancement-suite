{
  "name": "bitwig-enhancement-suite",
  "version": "1.0.0",
  "description": "Adds extra features to Bitwig",
  "scripts": {
    "electron": "electron dist/main/main.js",
    "watch": "tsc -w --p tsconfig.server.json",
    "start": "NODE_ENV=dev nodemon -V --watch ./dist . --exec 'npm run electron'",
    "dev": "webpack-dev-server",
    "build:mac": "PLATFORM=darwin ARCH=x64 node ./scripts/package.js && open ../producer-tools-app",
    "build:windows": "PLATFORM=win32 ARCH=ia32 node ./scripts/package.js && open ../producer-tools-app",
    "upload": "echo \"Update message?\" && read message && ./scripts/upload.sh \"$message\"",
    "package:mac": "PLATFORM=darwin npm run writeversion && npm run build:renderer && npm run build:main && npm run build:mac",
    "package:windows": "PLATFORM=win32 npm run writeversion && npm run build:renderer && npm run build:main && npm run build:windows",
    "packageandupload": "npm run package:mac && npm run package:windows && npm run upload",
    "build:renderer": "webpack",
    "build:main:alt": "webpack --config webpack.server.js",
    "build:main": "tsc --p tsconfig.server.json",
    "writeversion": "echo \"Version? Current is $npm_package_version\" && read VERSION && node ./scripts/writeVersion.js $VERSION",
    "buildc:dev": "node-gyp -j 16 build --debug",
    "buildc": "node-gyp -j 16 build",
    "rebuildc:dev": "node-gyp -j 16 rebuild --debug",
    "rebuildc": "node-gyp -j 16 rebuild",
    "cleanc": "node-gyp clean"
  },
  "build": {
    "extraResources": ["./extraResources/**"]
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "author": "Andrew Shand",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.1.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.6",
    "bindings": "^1.5.0",
    "electron-log": "^3.0.8",
    "electron-squirrel-startup": "^1.0.0",
    "electron-unhandled": "^3.0.0",
    "electron-util": "^0.12.2",
    "fuzzyset.js": "0.0.8",
    "is-electron-renderer": "^2.0.1",
    "moment": "^2.24.0",
    "node-addon-api": "^3.0.0",
    "react": "^16.10.2",
    "react-beautiful-dnd": "^11.0.5",
    "react-contextmenu": "^2.11.0",
    "react-dom": "^16.10.2",
    "react-loader-spinner": "^3.1.4",
    "react-router-dom": "^5.1.2",
    "request": "^2.88.0",
    "semver": "^6.3.0",
    "styled-components": "^4.4.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-hot-loader": "^3.0.5",
    "@types/webpack-env": "^1.13.2",
    "babel-loader": "^8.0.2",
    "babel-plugin-styled-components": "^1.7.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.11",
    "electron": "^6.0.12",
    "electron-packager": "^14.0.6",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "import-glob-loader": "^1.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.19.3",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.7",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "svg-react-loader": "^0.4.6",
    "ts-loader": "^6.2.0",
    "tslint": "^5.8.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.1.1",
    "webpack-node-externals": "^1.7.2"
  },
  "gypfile": true
}
